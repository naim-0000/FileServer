{
  "name": "net.ts",
  "version": "1.1.2",
  "description": "Node Express Template written in TypeScript NET.ts based on nodebestpractices",
  "main": "src/server.ts",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "build": "webpack",
    "server:prod": "node dist/server.js",
    "start": "node dist/server.js",
    "server:dev": "ts-node-dev --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest -c jest.config.js --env=node",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "prettier": "prettier --check \"{*.js,!(node*)**/*.ts}\"",
    "prettier:fix": "prettier --write \"{*.js,!(node*)**/*.ts}\"",
    "loglevel:change": "runtime-node-refresh"
  },
  "keywords": [
    "express template",
    "node template",
    "node express template",
    "node express template ts",
    "typescript template",
    "express",
    "boilerplate",
    "best practices node",
    "boilerplate node",
    "node ts template"
  ],
  "author": "nowicki.przemek@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "apicache": "^1.6.3",
    "axios": "^1.6.7",
    "compose": "^0.1.2",
    "cors": "^2.8.5",
    "docker": "^0.2.14",
    "dotenv": "^14.3.2",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-mongo-sanitize": "^2.2.0",
    "fs": "^0.0.1-security",
    "http-status": "^1.5.0",
    "joi": "^17.5.0",
    "lusca": "^1.7.0",
    "md5": "^2.3.0",
    "mongoose": "^7.6.8",
    "morgan": "^1.10.0",
    "node-mocks-http": "^1.11.0",
    "path": "^0.12.7",
    "runtime-node-refresh": "^1.0.6",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "ts-jest": "^27.1.1",
    "tsc-alias": "^1.5.0",
    "tslib": "^2.6.2",
    "typescript": "^4.5.2",
    "uuid": "^9.0.0",
    "web3": "^4.5.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^3.0.3",
    "prettier": "^2.5.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
